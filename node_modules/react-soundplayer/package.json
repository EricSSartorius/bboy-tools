{
  "name": "react-soundplayer",
  "version": "0.2.3",
  "description": "Create custom SoundCloud players with React",
  "main": "index.js",
  "directories": {
    "example": "examples"
  },
  "scripts": {
    "build-js": "rm -rf ./lib && babel src -d lib --stage 0",
    "build-css": "rm -rf ./styles && mkdir styles && node .cssnext",
    "build": "npm run build-js && npm run build-css",
    "prepublish": "npm run build"
  },
  "keywords": [
    "react",
    "component",
    "react-component",
    "soundcloud",
    "html5",
    "audio"
  ],
  "author": "Dmitri Voronianski <dmitri.voronianski@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "classnames": "^2.1.2",
    "object-assign": "^4.0.1",
    "react": "^0.13.3",
    "soundcloud-audio": "^0.1.6"
  },
  "devDependencies": {
    "babel": "^5.1.2",
    "babelify": "^6.0.2",
    "basscss": "^6.0.0",
    "browserify": "^9.0.8",
    "cssnext": "^1.2.3",
    "envify": "^3.4.0",
    "highlight.js": "^8.5.0",
    "watchify": "^2.4.0"
  }
}
